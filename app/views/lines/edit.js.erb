var slat = <%= params[:slat] %>
var slng = <%= params[:slng] %>
var elat = <%= params[:elat] %>
var elng = <%= params[:elng] %>
var id = <%= @line.id %>
$("#lines").find('.line').each(function(){
  if($(this).data('lid') == id){
    $(this).data('slat',slat);
    $(this).data('slng',slng);
    $(this).data('elat',elat);
    $(this).data('elng',elng); 
  }
});

